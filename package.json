{
  "name": "anylogger-console",
  "version": "1.1.0-beta.4",
  "type": "module",
  "description": "Anylogger adapter for the console",
  "exports": {
    ".": {
      "require": "./anylogger-console.cjs",
      "import": "./anylogger-console.js"
    }
  },
  "main": "./anylogger-console.js",
  "src": "./anylogger-console.ts",
  "cjs": "./anylogger-console.cjs",
  "iife": "./anylogger-console.iife.js",
  "min": "./anylogger-console.min.js",
  "spec": "./anylogger-console.spec.js",
  "files": [
    "anylogger-console.cjs",
    "anylogger-console.d.ts",
    "anylogger-console.d.ts.map",
    "anylogger-console.iife.js",
    "anylogger-console.js",
    "anylogger-console.js.map",
    "anylogger-console.min.js",
    "anylogger-console.spec.js",
    "anylogger-console.ts",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "unpkg": "anylogger-console.min.js",
  "scripts": {
    "build": "tsc && npm run test -s && npm run minify -s && npm run docs -s",
    "clean": "rimraf anylogger-console.cjs anylogger-console.d.ts anylogger-console.d.ts.map anylogger-console.iife.js anylogger-console.js anylogger-console.js.map anylogger-console.min.js",
    "docs": "cross-env NODE_ENV=production node build.js docs",
    "minify": "cross-env NODE_ENV=production node build.js minify",
    "package": "cross-env NODE_ENV=production rollup -c",
    "prepare": "npm run build",
    "test": "npm run package -s && mocha anylogger-console.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/download/anylogger-console.git"
  },
  "keywords": [
    "anylogger",
    "console",
    "anylogger-console",
    "log"
  ],
  "author": "Stijn de Witt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/download/anylogger-console/issues"
  },
  "homepage": "https://github.com/download/anylogger-console#readme",
  "peerDependencies": {
    "anylogger": "1.x || >=1.1.0-beta || >=1.2.0-beta || >=1.3.0-beta || >=1.4.0-beta || >=1.5.0-beta || >=1.6.0-beta || >=1.7.0-beta || >=1.8.0-beta || >=1.9.0-beta"
  },
  "devDependencies": {
    "anylogger": "^1.1.0-beta.0",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "gzip-size": "^7.0.0",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-re": "^1.0.7",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  }
}
